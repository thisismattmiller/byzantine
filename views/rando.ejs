<!DOCTYPE html>
<html lang="en">
  <head>
    <title>Welcome to Glitch!</title>
    <meta name="description" content="A cool thing made with Glitch">
    <link id="favicon" rel="icon" href="https://glitch.com/edit/favicon-app.ico" type="image/x-icon">
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!-- import the webpage's stylesheet -->
    <link rel="stylesheet" href="/style.css">
    
    <!-- import the webpage's client-side javascript file -->
    <script src="/client.js" defer></script>
    <link href="https://fonts.googleapis.com/css?family=VT323" rel="stylesheet">
  </head>
  <body>
    
    
    

   
    <style>
      #pdf{
        overflow:hidden;     
            position: relative;

      }

    
    </style>
    
    
    
    <div id="pdf">

      
    </div>
    
    <script>
        
      var rando = <%-rando%>
      

      function insert(imgsrc)
      {
          var imgDestination = document.getElementById("pdf");
          var imgAdded = document.createElement("img");
          imgAdded.src = imgsrc;
          imgDestination.appendChild(imgAdded);
          ImgRandomPosition(imgAdded);
      }


      function ImgRandomPosition(imgAdded)
      {
          var left = Math.floor((Math.random() * 400) + 1)+"px";
          var top = Math.floor((Math.random() * 400) + 1)+"px";
          var imagestyle = imgAdded.style;
          imagestyle.position = "absolute";
          imagestyle.width = "20%";

        imagestyle.top = top;
          imagestyle.left = left;
      }
      rando.forEach((img)=>{
        insert('https://s3.amazonaws.com/byzantine-pdf/images/'+img)
      })
      
      
      
    </script>
    
    
    

    <!-- include the Glitch button to show what the webpage is about and
          to make it easier for folks to view source and remix -->
    <div class="glitchButton" style="position:fixed;top:20px;right:20px;"></div>
    <script src="https://button.glitch.me/button.js"></script>

  </body>
</html>
